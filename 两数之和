class Solution(object):
    def twoSum(self, nums, target):
        """
        :type nums: List[int]
        :type target: int
        :rtype: List[int]
        """
        d = {}
        i=0
        for v in nums:
            if v in d:
               return [d[v],i]
            d[target-v]=i
            i+=1
